pipeline{


  agent any
  
    stages{
    
      stage('GitCheckout'){
      
      steps{
      
        script{
        
        
           git branch: 'main', url: 'https://github.com/sbhore01/CI.git'
    
     }
    
    }
   
   }
   
    stage('Maven Test'){
    
      steps{
       script{
    
    sh ' mvn test'
      
      }
     }
    }
     stage('Int.testing'){
      steps{
        script{
        
         sh 'mvn verify -DskipUnitTests'
        
        
        
        }
      }
     }
  }

}
